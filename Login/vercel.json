{
    "builds": [
        {
            "src": "cadastro/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.3" }
        }
    ],
    "routes": [
        {
            "src": "cadastro_app/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "cadastro/wsgi.py"
        }
    ]
}
