{
  "builds": [
    {
      "src": "cadastro/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "cadastro/wsgi.py"
    }
  ],
  "scripts": {
    "build": "python manage.py collectstatic --noinput"
  }
}

