{
  "version": 2,
  "builds": [
    {
      "src": "{Projeto_Arquitetura_de_Software}/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "Python3.12.3"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "{Projeto_Arquitetura_de_Software}/wsgi.py"
    }
  ]
}
